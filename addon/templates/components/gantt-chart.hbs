<div class="gantt-chart-inner">

  {{! timeline - header and grid }}
  <GanttTimeline @chart={{this}} as |timeline|>
    <timeline.header>{{yield (hash header=(component "gantt-generic-content" )) }}</timeline.header>
  </GanttTimeline>

  {{! lines and (recursive) sublines }}
  <div class="gantt-titles-background"></div>
  <div class="gantt-lines">
    {{yield (hash line=(component "gantt-line" chart=this )) }}
  </div>

</div>